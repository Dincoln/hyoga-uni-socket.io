#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ydj/Downloads/hyoga-uni-socket.io/lib/engine.io-client/node_modules/.pnpm/yamljs@0.1.4/node_modules/yamljs/bin/node_modules:/Users/ydj/Downloads/hyoga-uni-socket.io/lib/engine.io-client/node_modules/.pnpm/yamljs@0.1.4/node_modules/yamljs/node_modules:/Users/ydj/Downloads/hyoga-uni-socket.io/lib/engine.io-client/node_modules/.pnpm/yamljs@0.1.4/node_modules:/Users/ydj/Downloads/hyoga-uni-socket.io/lib/engine.io-client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ydj/Downloads/hyoga-uni-socket.io/lib/engine.io-client/node_modules/.pnpm/yamljs@0.1.4/node_modules/yamljs/bin/node_modules:/Users/ydj/Downloads/hyoga-uni-socket.io/lib/engine.io-client/node_modules/.pnpm/yamljs@0.1.4/node_modules/yamljs/node_modules:/Users/ydj/Downloads/hyoga-uni-socket.io/lib/engine.io-client/node_modules/.pnpm/yamljs@0.1.4/node_modules:/Users/ydj/Downloads/hyoga-uni-socket.io/lib/engine.io-client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../yamljs/bin/json2yaml" "$@"
else
  exec node  "$basedir/../yamljs/bin/json2yaml" "$@"
fi
