#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ydj/Downloads/hyoga-uni-socket.io/lib/engine.io-client/node_modules/.pnpm/watchify@3.7.0/node_modules/watchify/bin/node_modules:/Users/ydj/Downloads/hyoga-uni-socket.io/lib/engine.io-client/node_modules/.pnpm/watchify@3.7.0/node_modules/watchify/node_modules:/Users/ydj/Downloads/hyoga-uni-socket.io/lib/engine.io-client/node_modules/.pnpm/watchify@3.7.0/node_modules:/Users/ydj/Downloads/hyoga-uni-socket.io/lib/engine.io-client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ydj/Downloads/hyoga-uni-socket.io/lib/engine.io-client/node_modules/.pnpm/watchify@3.7.0/node_modules/watchify/bin/node_modules:/Users/ydj/Downloads/hyoga-uni-socket.io/lib/engine.io-client/node_modules/.pnpm/watchify@3.7.0/node_modules/watchify/node_modules:/Users/ydj/Downloads/hyoga-uni-socket.io/lib/engine.io-client/node_modules/.pnpm/watchify@3.7.0/node_modules:/Users/ydj/Downloads/hyoga-uni-socket.io/lib/engine.io-client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../watchify/bin/cmd.js" "$@"
else
  exec node  "$basedir/../watchify/bin/cmd.js" "$@"
fi
